{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@onboarding/shared/data-access": [
        "libs/shared/data-access/src/index.ts"
      ],
      "@onboarding/shared/domain": ["libs/shared/domain/src/index.ts"],
      "@onboarding/shared/ui-error": ["libs/shared/ui-error/src/index.ts"],
      "@onboarding/shared/ui-loading": ["libs/shared/ui-loading/src/index.ts"],
      "@onboarding/shared/ui-recipe-list-item": [
        "libs/shared/ui-recipe-list-item/src/index.ts"
      ],
      "@onboarding/shared/util": ["libs/shared/util/src/index.ts"],
      "@onboarding/shared/util-click-stop-propagation-directive": [
        "libs/shared/util-click-stop-propagation-directive/src/index.ts"
      ],
      "@onboarding/web/recipes/recipes-list/data-access": [
        "libs/web/recipes/recipes-list/data-access/src/index.ts"
      ],
      "@onboarding/web/recipes/recipes-list/feature": [
        "libs/web/recipes/recipes-list/feature/src/index.ts"
      ],
      "@onboarding/web/recipes/recipes-list/ui-recipes-filter": [
        "libs/web/recipes/recipes-list/ui-recipes-filter/src/index.ts"
      ],
      "@onboarding/web/recipes/recipes-list/util": [
        "libs/web/recipes/recipes-list/util/src/index.ts"
      ],
      "@onboarding/web/recipes/shell": ["libs/web/recipes/shell/src/index.ts"],
      "@onboarding/web/shared/navbar/feature": [
        "libs/web/shared/navbar/feature/src/index.ts"
      ],
      "@onboarding/web/shared/navbar/ui": [
        "libs/web/shared/navbar/ui/src/index.ts"
      ],
      "@onboarding/web/shell": ["libs/web/shell/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
